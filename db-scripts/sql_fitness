DROP table fitness;

CREATE TABLE fitness
     (
     id INT PRIMARY KEY AUTO_INCREMENT,
     nazev VARCHAR(50),
     url VARCHAR(70),
     ulice VARCHAR(30),
     mesto VARCHAR(20),
     psc INT(5),
     multisport BOOLEAN,
     activepass BOOLEAN,
     platba VARCHAR(30),
     pohlavi VARCHAR(10),
     mhd BOOLEAN
     );


